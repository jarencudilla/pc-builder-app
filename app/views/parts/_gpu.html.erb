<section class="table-container">
  <table class="table">
    <Caption>Graphic Cards</Caption>

    <thead>  
      <tr>
        <th>Image</th>
        <th>Name</th>
        <th>Chipset</th>
        <th>Core Clock</th>
        <th>Boost Clock</th>
        <th>Color</th>
        <th>length</th>
        <th>Rating Socket</th>
        <th>Memory Size</th>
        <th>Memory Type</th>
        <th>Price</th>
      </tr>
    </thead>

    <tbody>
      <% @gpus.each do |g| %>
        <tr>
          <td><%= g.image %></td>
          <td><%= g.name %></td>
          <td><%= g.chipset %></td>
          <td><%= g.core_clock %></td>
          <td><%= g.boost_clock %></td>
          <td><%= g.color %></td>
          <td><%= g.length %></td>
          <td><%= g.rating %></td>
          <td><%= g.memory_size %></td>
          <td><%= g.memory_type %></td>
          <td><%= g.price %></td>
        </tr>
      <%end%>
    </tbody>
  </table>
</section>
