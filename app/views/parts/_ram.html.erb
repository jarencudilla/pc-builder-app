<section class="table-container">
  <table class="table">
    <Caption>RAMS</Caption>

    <thead>  
      <tr>
        <th>Image</th>
        <th>Name</th>
        <th>Speed</th>
        <th>Latency</th>
        <th>Color</th>
        <th>Rating</th>
        <th>Memory Type</th>
        <th>Memory Size</th>
        <th>Price</th>
      </tr>
    </thead>

    <tbody>
      <% @rams.each do |ram| %>
        <tr>
          <td><%= ram.image %></td>
          <td><%= ram.name %></td>
          <td><%= ram.speed %></td>
          <td><%= ram.latency %></td>
          <td><%= ram.color %></td>
          <td><%= ram.rating %></td>
          <td><%= ram.memory_type %></td>
          <td><%= ram.memory_size %></td>
          <td><%= ram.price %></td>
        </tr>
      <%end%>
    </tbody>
  </table>
</section>
